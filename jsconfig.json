{
  "vueCompilerOptions": {
    "target": 2.7,
    "extensions": [
      ".vue"
    ]
  },
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "@dcloudio/types",
      "miniprogram-api-typings",
      "mini-types"
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@mixins/*": ["./src/mixins/*"],
      "@utils/*": ["./src/utils/*"],
      "@api/*": ["./src/api/*"],
      "@store/*": ["./src/store/*"],
      "@pages/*": ["./src/pages/*"],
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"]
    }
  }
}
